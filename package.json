{
  "name": "@kevinmarrec/eslint-config",
  "type": "module",
  "version": "1.4.0",
  "packageManager": "bun@1.2.22",
  "description": "Opinionated ESLint config.",
  "author": "Kevin Marrec <kevin@marrec.io>",
  "license": "MIT",
  "homepage": "https://github.com/kevinmarrec/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinmarrec/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "bun run check:unused && bun run check:eslint && bun run check:types",
    "check:eslint": "eslint . --cache",
    "check:types": "tsc --noEmit",
    "check:unused": "knip -n",
    "lint": "bun run check:eslint",
    "lint:inspect": "bunx @eslint/config-inspector",
    "release": "bumpp",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "eslint": ">=9.20.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^5.4.0",
    "@unocss/eslint-plugin": "^66.5.1",
    "defu": "^6.1.4",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "tinyglobby": "^0.2.15"
  },
  "devDependencies": {
    "@kevinmarrec/tsconfig": "^1.1.0",
    "@types/bun": "^1.2.22",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "^10.2.3",
    "eslint": "^9.36.0",
    "knip": "^5.63.1",
    "pathe": "^2.0.3",
    "taze": "^19.7.0",
    "tsdown": "^0.15.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
